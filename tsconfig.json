{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "sourceMap": true,
    "rootDir": "backend_inte/src", 
    "outDir": "./dist",
    "removeComments": false,
    "importsNotUsedAsValues": "remove",
    "stripInternal": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "typeRoots": ["backend_inte/src/types"],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "include": ["./.eslintrc.js", "backend_inte/src", "lib/try.ts"]
}